{
  "family": "sample-fargate", 
  "networkMode": "awsvpc", 
  "containerDefinitions": [
      {
          "name": "bad-spelling-generator", 
          "image": "${{ secrets.ECR_REPOSITORY }}", 
          "portMappings": [
              {
                  "containerPort": 5000, 
                  "protocol": "tcp"
              }
          ], 
          "essential": true, 
          "entryPoint": ["python"], 
          "command": ["app.py"]
      }
  ], 
  "executionRoleArn": "${{ secrets.EXECUTION_ROLE_ARN }}",
  "requiresCompatibilities": [
      "FARGATE"
  ], 
  "cpu": "256", 
  "memory": "512"
}